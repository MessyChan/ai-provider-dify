{
  "name": "ai-provider-dify",
  "version": "1.0.2",
  "description": "Dify provider for Vercel AI SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "ai",
    "dify",
    "vercel-ai-sdk",
    "ai-provider-dify"
  ],
  
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "^3.0.4",
    "@ai-sdk/provider-utils": "^4.0.8",
    "ai": "^6.0.41",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  }
}
